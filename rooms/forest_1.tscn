[sub_resource type="GDScript" id="GDScript_4mv6m"]
script/source = "extends Room

var solved = false

func _ready() -> void:
	if Globals.room_transition:
		await Globals.room_transition_finished
		$PlayerCharacterBody.position = Globals.player_position
	
	spawn_clones($PlayerCharacterBody, clone_multiplier, 1)
	
	if solved:
		unlock_door()
		set_physics_process(false)

func neighbour_rooms(side: int) -> String:
	if side == SIDE_LEFT:
		return \"res://rooms/seaside_1.tscn\"
	return super.neighbour_rooms(side)

func _physics_process(delta: float) -> void:
	var buttons = [$Button, $Button2, $Button3, $Button4]
	var solution = buttons.all(func(button): return button.pressed)
	
	if solution:
		solved = true
		unlock_door_anim()
		set_physics_process(false)

func unlock_door():
	for x in range(14, 18):
		$TileMap.erase_cell(0, Vector2i(x, 3))
		$TileMap.erase_cell(0, Vector2i(x, 4))

func unlock_door_anim():
	for x in range(14, 18):
		await get_tree().create_timer(.7).timeout
		$TileMap.erase_cell(0, Vector2i(x, 3))
		$TileMap.erase_cell(0, Vector2i(x, 4))
"

